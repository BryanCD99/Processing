// Clase Grid que divide el mapa en una matriz de tamaño w,h personalizable, con areas cuadradas ajustables en gridsize
// La clase permite subdividir un area o un mapa en zonas que facilitan la identificacion de objetos, muros y entidades para el raycasting
class Grid{
  public int gridsize;
  public int width;
  public int height;
  private Object[][] Entities;

  
  // Crea un grid de tamaño w,h con un area gridsize x gridsize
  public Grid(int gridsize, int h, int w){
    this.gridsize = gridsize;
    this.width = w;
    this.height = h;
    this.Entities = new Object[h][w];
  }
  // Coloca un objeto en el grid
  public void setEntity(int j, int i, Object Entity) {
    this.Entities[j][i] = Entity;
  }
  // Obtiene el objeto del grid
  public Object getEntity(int j, int i) {
    return this.Entities[j][i];
  }
  // Elimina un objeto del grid
  public void ClearEntity(int j, int i){
    this.Entities[j][i] = null;
  }
  // Pregunta si el objeto existe
  public boolean containsObject(int j, int i){
    return this.Entities[j][i] != null;
  }
  // Pregunta si dos grids contienen el mismo objeto
  public boolean istheSameObject(int j0, int i0,int j1, int i1){
    return this.Entities[j0][i0] == this.Entities[j1][i1];
  }  
  // Determina si un objeto esta invadiendo un grid y lo actualiza (Nota: Es posible que los grids ubicados a las esquinas del grid analizado no detecten el objeto debido a que puede
                                                                   // ocurrir que ningun punto analizado se encuentre en el area en determinadas situaciones)
  public void updateGrid(Object Entity) {
    // Obtener la posición del centro del muro
    if(Entity == null){
      return;
    }
    if (Entity instanceof Muro){
      Muro CurrrentMuro = (Muro)Entity;
      float x = CurrrentMuro.x;
      float y = CurrrentMuro.y;

      // Calcular el índice del grid en el que se encuentra el centro del muro
      int i = floor(x/this.gridsize);
      int j = floor(y/this.gridsize);
      // Calcular las posiciones de los puntos del muro según su ángulo
      float halfBlockcosAngle = (CurrrentMuro.blockSize/2)*cos(CurrrentMuro.angle);
      float halfBlocksinAngle = -(CurrrentMuro.blockSize/2)*sin(CurrrentMuro.angle);
      
      // Calcula los puntos del muro analizado
      float x0_rotated = x - halfBlockcosAngle - halfBlocksinAngle;
      float y0_rotated = y - halfBlocksinAngle + halfBlockcosAngle;
      
      float x1_rotated = x + halfBlockcosAngle - halfBlocksinAngle;
      float y1_rotated = y + halfBlocksinAngle + halfBlockcosAngle;
      
      float x2_rotated = x + halfBlockcosAngle + halfBlocksinAngle;
      float y2_rotated = y + halfBlocksinAngle - halfBlockcosAngle;
      
      float x3_rotated = x - halfBlockcosAngle + halfBlocksinAngle;
      float y3_rotated = y - halfBlocksinAngle - halfBlockcosAngle;
      
      // Recorrer los grids vecinos y verificar si el muro está invadiendo alguno de ellos
      for (int di = -1; di <= 1; di++) {
          for (int dj = -1; dj <= 1; dj++) {
              int ni = i + di;
              int nj = j + dj;
              if (ni >= 0 && ni < this.width && nj >= 0 && nj < this.height) { 
                // Agregar una referencia al muro en el grid correspondiente si este esta vacio
                if ((!this.containsObject(nj, ni))||(this.istheSameObject(j,i,nj,ni))){  
                  // Calcular los límites del grid
                  float x0 = ni * this.gridsize;
                  float y0 = nj * this.gridsize;
                  float x1 = (ni + 1) * this.gridsize;
                  float y1 = (nj + 1) * this.gridsize;  
                 
    
                  // Verificar si alguno de los puntos del muro está invadiendo el grid
                  if (x0_rotated >= x0 && x0_rotated <= x1 && y0_rotated >= y0 && y0_rotated <= y1 ||
                      x1_rotated >= x0 && x1_rotated <= x1 && y1_rotated >= y0 && y1_rotated <= y1 ||
                      x2_rotated >= x0 && x2_rotated <= x1 && y2_rotated >= y0 && y2_rotated <= y1 ||
                      x3_rotated >= x0 && x3_rotated <= x1 && y3_rotated >= y0 && y3_rotated <= y1) {                        
                      // Coloca la referencia correspondiente al grid en la posicion configurada                                           
                      this.setEntity(nj, ni, CurrrentMuro);
                  }else{
                    // Si un objeto sale del area de un grid aledaño, se borra la referencia en este grid y se libera
                    if (i!=ni&&j!=nj&&this.istheSameObject(j,i,nj,ni))
                    {
                      this.ClearEntity(nj, ni);
                    }

                  }
                } 

                  //// Verificar si el muro está invadiendo el grid
                  //if (x + CurrrentMuro.blockSize/2 > x0 && x - CurrrentMuro.blockSize/2 < x1 && y + CurrrentMuro.blockSize/2 > y0 && y - CurrrentMuro.blockSize/2 < y1) {
                  //    // Agregar una referencia al muro en el grid correspondiente si este esta vacio
                  //    if(!(this.containsObject(nj,ni))){
                  //      this.setEntity(nj,ni,CurrrentMuro);
                  //    }
                  //}
              }
          }
      }
    }   
  }

};

// Clase muro que contiene todos los parametros y metodos para colocar, configurar y ubicar un muro en el raycasting 
class Muro {
  public float x, y;    // Localizacion del objeto en el mapa (Esta localizacion puede ser cualquier lugar del grid y no esta limitado por este
  float blockSize;      // Tamaño del muro
  float angle;          // Orientacion del muro
  String[] textures = new String[4];    // Textura para cada lado del muro cuadrado

  // Inicializa un muro cuadrado en una posicion x,y de un tamaño blocksize y orientado en angle
  Muro(float x, float y, float blockSize, float angle) {
    this.x = x;
    this.y = y;
    this.blockSize = blockSize;
    this.angle = angle;
  }

  // Configura una textura (string que indica cual es la textura aplicada) para cada lado del muro
  void setTextura(String textura, int lado) {
    textures[lado] = textura;
  }
  
  // Obtiene la textura aplicada en lado especificado 
  String getTexture(int lado) {
    return textures[lado];
  }

  // Determina si un punto x,y se encuentra dentro del muro
  boolean isPointInside(float xPoint, float yPoint) {
    // Rota el punto de acuerdo al angulo de orientacion del muro
    float rotatedX = this.x + (xPoint - this.x) * cos(angle) - (yPoint - this.y) * sin(angle);
    float rotatedY = this.y + (xPoint - this.x) * sin(angle) + (yPoint - this.y) * cos(angle);

    // Si el punto se encuentra dentro del area del muro, retorna true
    return rotatedX >= this.x  - this.blockSize / 2 && rotatedX <= this.x + this.blockSize / 2 && rotatedY >= this.y - this.blockSize / 2 && rotatedY <= this.y + this.blockSize / 2;  
  }

  // Obtiene el lado del muro mas cercano al punto especificado *********
  String getClosestSide(float xPoint, float yPoint) {
    if (!this.isPointInside(xPoint,yPoint)){
      return "Unknow";
    }
    // Rota el punto de acuerdo al angulo de orientacion del muro
    float rotatedX = this.x + (xPoint - this.x) * cos(angle) - (yPoint - this.y) * sin(angle);
    float rotatedY = this.y + (xPoint - this.x) * sin(angle) + (yPoint - this.y) * cos(angle);
    
    float distTop = dist(rotatedX, rotatedY, this.x, this.y + blockSize / 2);
    float distBottom = dist(rotatedX, rotatedY, this.x, this.y - blockSize / 2);
    float distLeft = dist(rotatedX, rotatedY, this.x + blockSize / 2, this.y);
    float distRight = dist(rotatedX, rotatedY, this.x - blockSize / 2, this.y);
    float minDist = min(distTop, distBottom, min(distLeft, distRight));
    if (minDist == distTop) {
      return "U";
    } else if (minDist == distBottom) {
      return "B";
    } else if (minDist == distLeft) {
      return "L";
    } else {
      return "R";
    }
  }
};
